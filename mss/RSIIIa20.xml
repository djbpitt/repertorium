                <scribeDesc xmlns="http://www.ilit.bas.bg/repertorium/ns/3.0">
                    <scribe>
                        <name xmlns="http://www.tei-c.org/ns/1.0" full="yes">anonymous</name>
                        <scriptDesc xmlns="http://www.tei-c.org/ns/1.0" script="Cyrs" type="semiuncial"/>
                        <scribeLang>
                            <orthography>
                                <p xmlns="http://www.tei-c.org/ns/1.0">Serbian orthography</p>
                            </orthography>
                        </scribeLang>
                        <note xmlns="http://www.tei-c.org/ns/1.0">
                            <ref type="bibl" target="bib:Mošin1955">Mošin 1955: 74-75</ref>
                        </note>
                    </scribe>
