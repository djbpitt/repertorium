                        <locus>97-104</locus>
                        <name full="yes">anonymous</name>
                        <re:scribeLang script="Cyrs" type="semiuncial"/>
                        <note>
                            <date>19th c.</date>
                            <ref type="bibl" target="bib:Штављанин-Ђорђевић1986">Штављанин-Ђорђевић,
                                Гроздановић-Паић, Цернић 1986.1: 68-72</ref>
