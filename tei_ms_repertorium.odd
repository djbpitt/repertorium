                    <elementSpec ident="msItemStruct" module="msdescription" mode="change">
                        <content>
                            <rng:group>
                                <rng:choice>
                                    <rng:optional>
                                        <rng:ref name="locus"/>
                                    </rng:optional>
                                    <rng:optional>
                                        <rng:ref name="locusGrp"/>
                                    </rng:optional>
                                </rng:choice>
                                <rng:choice>
                                    <rng:oneOrMore>
                                        <rng:ref name="model.pLike"/>
                                    </rng:oneOrMore>
                                    <rng:group>
                                        <rng:zeroOrMore>
                                            <rng:ref name="title"/>
                                        </rng:zeroOrMore>
                                        <rng:zeroOrMore>
                                            <rng:ref name="author"/>
                                        </rng:zeroOrMore>
                                        <rng:zeroOrMore>
                                            <rng:ref name="respStmt"/>
                                        </rng:zeroOrMore>
                                        <rng:zeroOrMore>
                                            <rng:ref name="model.noteLike"/>
                                        </rng:zeroOrMore>
                                        <rng:zeroOrMore>
                                            <rng:ref name="filiation"/>
                                        </rng:zeroOrMore>
                                        <rng:optional>
                                            <rng:ref name="textLang"/>
                                        </rng:optional>
                                        <rng:zeroOrMore>
                                            <rng:ref name="decoNote"/>
                                        </rng:zeroOrMore>
                                        <rng:zeroOrMore>
                                            <rng:ref name="colophon"/>
                                        </rng:zeroOrMore>
                                        <rng:optional>
                                            <rng:choice>
                                                <rng:ref name="sampleText"/>
                                                <rng:ref name="div"/>
                                            </rng:choice>
                                        </rng:optional>
                                        <rng:zeroOrMore>
                                            <rng:ref name="msItemStruct"/>
                                        </rng:zeroOrMore>
                                        <rng:zeroOrMore>
                                            <rng:ref name="listBibl"/>
                                        </rng:zeroOrMore>
                                        <rng:zeroOrMore>
                                            <rng:ref name="bibl"/>
                                        </rng:zeroOrMore>
                                    </rng:group>
                                </rng:choice>
                            </rng:group>
                        </content>
                        <attList>
                            <attDef ident="type">
                                <desc>describes the type of the text</desc>
                                <datatype>
                                    <rng:text/>
                                </datatype>
                                <valList type="closed">
                                    <valItem ident="original">
                                        <desc>the text is original</desc>
                                    </valItem>
                                    <valItem ident="compilation">
                                        <desc>the text is compiled from variuos sources</desc>
                                    </valItem>
                                    <valItem ident="translation">
                                        <desc>the text is translated</desc>
                                    </valItem>
                                </valList>
                            </attDef>
                        </attList>
                        <exemplum>
                            <p>TEI example: </p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples">
                                <msItemStruct n="2" defective="false" class="#biblComm">
                                    <locus from="24v" to="97v">24v-97v</locus>
                                    <author>Apringius de Beja</author>
                                    <title type="uniform" xml:lang="la">Tractatus in
                                        Apocalypsin</title>
                                    <rubric>Incipit Trac<supplied reason="omitted">ta</supplied>tus
                                        in apoka<lb/>lipsin eruditissimi uiri <lb/> Apringi
                                            ep<ex>iscop</ex>i Pacensis eccl<ex>esi</ex>e</rubric>
                                    <finalRubric>EXPLIC<ex>IT</ex> EXPO<lb/>SITIO APOCALIPSIS
                                            QVA<ex>M</ex> EXPOSVIT DOM<lb/>NVS APRINGIUS
                                            EP<ex>ISCOPU</ex>S. DEO GR<ex>ACI</ex>AS AGO.
                                        FI<lb/>NITO LABORE ISTO.</finalRubric>
                                    <bibl>
                                        <ref target="http://amiBibl.xml#Apringius1900"
                                            >Apringius</ref>, ed. Férotin</bibl>
                                    <textLang mainLang="la">Latin</textLang>
                                </msItemStruct>
                            </egXML>
                        </exemplum>
                        <exemplum>
                            <p>Repertorium makes significant changes in the content of this element.
                                A new attribute <att>type</att> with three possible values is added:
                                    <val>original</val>, <val>compilation</val>, and
                                    <val>translation</val>. Places of elements <gi>title</gi> and
                                    <gi>author</gi> are in different order: first is the title, and
                                then author. Elements <gi>incipit</gi>, <gi>explicit</gi>,
                                    <gi>rubric</gi> are part of the <gi>sampleText</gi> element,
                                which is part of the Repertorium namespace. We included here the
                                element <gi>div</gi> in cases where an edition of large part of
                                prviously unknown text or its recension is described. </p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples">
                                <msItemStruct xml:id="ACD1" type="translation">
                                    <locus n="1">1r-15v</locus>
                                    <title xml:lang="bg">Физиолог</title>
                                    <note>The copy represents first group of the second
                                        Physiologus's translation in South Slavic literature; other
                                        similar copies: No. 149 NB - Wien, 16th c.; No. 433 NBKM -
                                        Sofia, second half of the 16th c. <ref type="bibl"
                                            target="bib:Стойкова1994">Стойкова 1994: 53–54,
                                            70–71</ref>
                                    </note>
                                    <filiation type="source">
                                        <ref type="bibl" target="bib:Sbordone1936">Sbordone
                                            1936:218-221</ref> (approximate text)</filiation>
                                    <filiation type="translation">2</filiation>
                                    <filiation type="protograph">Bulgarian</filiation>
                                    <filiation type="antigraph">Antigraph of No. 433 and No.
                                        149?</filiation>
                                    <filiation type="litRedaction">1</filiation>
                                    <sampleText xmlns="http://www.ilit.bas.bg/repertorium/ns/3.0"
                                        xml:lang="cu">
                                        <head>Слово ѡ въсѣх вещеи ходещихъ и летещи<seg rend="sup"
                                                >х</seg>
                                        </head>
                                    </sampleText>
                                    <msItemStruct xml:id="ACD1.1">
                                        <locus n="1">1r-1v</locus>
                                        <title xml:lang="bg">Физиолог. За лъва</title>
                                        <sampleText
                                            xmlns="http://www.ilit.bas.bg/repertorium/ns/3.0"
                                            xml:lang="cu">
                                            <head>Слово за лъва о ц҃ри ѕвѣрстѣ<seg rend="sup"
                                                  >м</seg>.</head>
                                            <rubric>фисиолѡгь р<seg rend="sup">еч</seg>е ѡ лъвѣ.
                                                о҃че бл<seg rend="sup">с</seg>ви</rubric>
                                            <incipit>Лъвь ѥ<seg rend="sup">с</seg> имѣе дѣла велика
                                                три. къ с҃тѣи тр<seg rend="sup">о</seg>ици
                                                примолѥнь. лъво<seg rend="sup">в</seg> ѥ<seg
                                                  rend="sup">с</seg> пръво дѣло. егда раж<seg
                                                  rend="sup">д</seg>аеть лъвица.</incipit>
                                        </sampleText>
                                    </msItemStruct> ... </msItemStruct>
                            </egXML>
                        </exemplum>
                    </elementSpec>

                    <elementSpec ident="msPart" module="msdescription" mode="change">
                        <classes mode="change">
                            <memberOf key="att.typed" mode="add"/>
                        </classes>
                        <content>
                            <rng:group>
                                <rng:group>
                                    <rng:choice>
                                        <rng:zeroOrMore>
                                            <rng:optional>
                                                <rng:ref name="altIdentifier"/>
                                            </rng:optional>
                                            <rng:optional>
                                                <rng:ref name="msIdentifier"/>
                                            </rng:optional>
                                            <rng:optional>
                                                <rng:ref name="locus"/>
                                            </rng:optional>
                                            <rng:optional>
                                                <rng:ref name="locusGrp"/>
                                            </rng:optional>
                                        </rng:zeroOrMore>
                                    </rng:choice>
                                </rng:group>
                                <rng:zeroOrMore>
                                    <rng:ref name="model.headLike"/>
                                </rng:zeroOrMore>
                                <rng:choice>
                                    <rng:oneOrMore>
                                        <rng:ref name="model.pLike"/>
                                    </rng:oneOrMore>
                                    <rng:group>
                                        <rng:optional>
                                            <rng:ref name="physDesc"/>
                                        </rng:optional>
                                        <rng:optional>
                                            <rng:ref name="scribeDesc"/>
                                        </rng:optional>
                                        <rng:optional>
                                            <rng:ref name="msContents"/>
                                        </rng:optional>
                                        <rng:optional>
                                            <rng:ref name="history"/>
                                        </rng:optional>
                                        <rng:optional>
                                            <rng:ref name="additional"/>
                                        </rng:optional>
                                        <rng:zeroOrMore>
                                            <rng:ref name="msPart"/>
                                        </rng:zeroOrMore>
                                    </rng:group>
                                </rng:choice>
                            </rng:group>
                        </content>
                        <exemplum>
                            <p>TEI example:</p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples">
                                <msDesc>
                                    <msIdentifier>
                                        <settlement>Amiens</settlement>
                                        <repository>Bibliothèque Municipale</repository>
                                        <idno>MS 3</idno>
                                        <msName>Maurdramnus Bible</msName>
                                    </msIdentifier>
                                    <msContents>
                                        <summary xml:lang="lat">Miscellany of various texts;
                                            Prudentius, Psychomachia; Physiologus de natura
                                            animantium</summary>
                                        <textLang mainLang="lat">Latin</textLang>
                                    </msContents>
                                    <physDesc>
                                        <objectDesc form="composite_manuscript"/>
                                    </physDesc>
                                    <msPart>
                                        <msIdentifier>
                                            <idno>ms. 10066-77 ff. 140r-156v</idno>
                                        </msIdentifier>
                                        <msContents>
                                            <summary xml:lang="lat">Physiologus</summary>
                                            <textLang mainLang="lat">Latin</textLang>
                                        </msContents>
                                    </msPart>
                                    <msPart>
                                        <msIdentifier>
                                            <altIdentifier>
                                                <idno>MS 6</idno>
                                            </altIdentifier>
                                        </msIdentifier>
                                        <!-- other information specific to this part here -->
                                    </msPart>
                                    <!-- more parts here -->
                                </msDesc>
                            </egXML>
                        </exemplum>
                        <exemplum>
                            <p>Repertorium adds <att>type</att> attribute. The other changes include
                                posibilities to add different types of identifiers
                                    (<gi>msIdentifier</gi> and <gi>altIdentifier</gi>), and a
                                special Repertorium <gi>sampleText</gi> element. An example for
                                convolute (file AM!-108O.xml): </p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples">
                                <msPart n="1">
                                    <locus>f. 1</locus>
                                    <physDesc>
                                        <objectDesc>
                                            <supportDesc material="paper">
                                                <support>
                                                  <material>Paper</material>
                                                  <watermark>none</watermark>
                                                </support>
                                                <extent> ... </extent>
                                                <condition>folio seems to be added to the book
                                                  later</condition>
                                            </supportDesc>
                                            <layoutDesc> ... </layoutDesc>
                                        </objectDesc> ... </physDesc>
                                    <scribeDesc xmlns="http://www.ilit.bas.bg/repertorium/ns/3.0">
                                        ... </scribeDesc>
                                    <msContents>
                                        <msItemStruct xml:id="ACD1" type="translation">
                                            <locus n="1">1r-1v</locus>
                                            <title xml:lang="bg">Съдържание на втората книга на Паноплия догматики</title>
                                            <note>without end</note>
                                            <sampleText
                                                xmlns="http://www.ilit.bas.bg/repertorium/ns/3.0"
                                                xml:lang="cu">
                                                <head>Книга догматики паноплїе, втораа.</head>
                                                <incipit>Титле вторые книги, г҃и.</incipit>
                                                <explicit defective="true"> ег<seg rend="sup"
                                                  >д</seg>а ѿ гж<seg rend="sup">д</seg>е своѥе,
                                                  шьствоваше<gap/>
                                                </explicit>
                                            </sampleText>
                                        </msItemStruct>
                                    </msContents> ... </msPart>
                                <msPart n="2" xml:id="Gerasim">
                                    <locus>f. 2.</locus> ... </msPart>
                                <msPart n="3">
                                    <locus>ff. 3-35</locus> ... </msPart>
                                <msPart n="4">
                                    <locus>ff. 36-59</locus> ... </msPart>
                                <msPart n="5">
                                    <locus>ff. 61-72</locus> ... </msPart>
                            </egXML>
                        </exemplum>
                    </elementSpec>

                    <elementSpec ident="musicNotation" module="msdescription" mode="change">
                        <classes mode="replace">
                            <memberOf key="att.global"/>
                            <memberOf key="att.typed" mode="add"/>
                        </classes>
                        <content>
                            <rng:ref name="macro.specialPara"/>
                        </content>
                        <attList>
                            <attDef ident="system" usage="opt">
                                <datatype>
                                    <rng:text/>
                                </datatype>
                            </attDef>
                        </attList>
                        <exemplum>
                            <p>TEI example:</p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples">
                                <musicNotation>Neumes in <term>campo aperto</term> of the St. Gall
                                    type. </musicNotation>
                            </egXML>
                        </exemplum>
                        <exemplum>
                            <p/>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples"> An example to be
                                included !!! </egXML>
                        </exemplum>
                    </elementSpec>

                    <elementSpec ident="orthNote" ns="http://www.ilit.bas.bg/repertorium/ns/3.0"
                        mode="add">
                        <desc>Specifies a detail of the orthographic practice of a scribe.</desc>
                        <classes mode="replace">
                            <memberOf key="att.global"/>
                            <memberOf key="att.typed"/>
                            <memberOf key="att.naming"/>
                            <memberOf key="att.scoping"/>
                        </classes>
                        <content>
                            <rng:ref name="macro.specialPara"/>
                        </content>
                        <exemplum>
                            <p>In the example below, the <att>type</att> and <att>subtype</att>
                                attributes are used to indicate that the orthography uses only the
                                front jer. To describe jer orthography the value of the
                                    <att>type</att> attribute must be <val>jer</val>, in which case
                                there are five permitted <att>subtype</att> values:<list>
                                    <item><val>front</val>: only front jer is used</item>
                                    <item><val>back</val>: only back jer is used</item>
                                    <item><val>etymReg</val>: etymologically regular use of
                                        jers</item>
                                    <item><val>nonEtymReg</val>: not etymological but regular use of
                                        jers</item>
                                    <item><val>irregular</val>: the use of jers cannot be formalized
                                        in general rules</item>
                                </list></p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples"><orthNote type="jer"
                                    subtype="front">only <c xml:lang="cu">ь</c>
                                </orthNote></egXML>
                        </exemplum>
                        <exemplum>
                            <p>In the example below, the <att>type</att> and <att>subtype</att>
                                attributes are used to indicate that the orthography uses both front
                                and back nasals, but does so inconsistently. To describe nasal vowel
                                orthography the value of the <att>type</att> attribute must be
                                    <val>jus</val>, in which case there are five permitted
                                    <att>subtype</att> values:<list>
                                    <item><val>etymReg</val>: etymologically regular use of juses
                                        (e.g., as in Old Bulgarian)</item>
                                    <item><val>nonEtymReg</val>: non etymological, but otherwise
                                        regular use of the juses (Middle Bulgarian manuscripts, and
                                        in part East Slavic examples)</item>
                                    <item><val>nonConsis</val>: nonconsistent use of the
                                        juses</item>
                                    <item><val>nonJus</val>: orthography without juses</item>
                                    <item><val>jusTrace</val>: orthography without or with very few
                                        examples of the juses, but with traces, e.g., manuscripts
                                        written generally with non-jus orthography but with traces
                                        of the Middle Bulgarian alternation of juses</item>
                                </list></p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples"><orthNote type="jus"
                                    subtype="nonConsis"><c xml:lang="cu">ѫ</c> replaced by <c
                                        xml:lang="cu">оу</c>; <c xml:lang="cu">ѧ</c> replaced by <c
                                        xml:lang="cu">a</c>;</orthNote></egXML>
                        </exemplum>
                        <exemplum>
                            <p>In the example below, the <att>type</att> attribute with the value of
                                    <val>jotVowel</val>indicates that this note pertains to
                                jotation. In this case there is no <att>subtype</att> attribute, and
                                the behavior is described only in prose.</p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples"><orthNote
                                    type="jotVowel">The letters <c xml:id="cu">є</c> and <c
                                        xml:id="cu">ѥ</c> are both used in intervocalic
                                    position.</orthNote></egXML>
                        </exemplum>
                        <exemplum>
                            <p>The <att>type</att> attribute value of <val>otherLetters</val>
                                indicates that this note pertains to letters other than jers,
                                nasals, and jotation.</p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples"><orthNote
                                    type="otherLetters"><c xml:lang="cu">ы</c>; <c xml:lang="cu"
                                        >ѥ</c> and <c xml:lang="cu">ѩ</c></orthNote></egXML>
                        </exemplum>
                        <remarks>
                            <p>The <att>type</att> and <att>subtype</att> attributes support
                                retrieval of manuscripts according to the most important
                                characteristics of scribal practice. The four permitted
                                    <att>type</att> values are: <list>
                                    <item><val>jer</val>: jer letters</item>
                                    <item><val>jus</val>: nasal vowel letters</item>
                                    <item><val>jotVowel</val>: jotation</item>
                                    <item>and <val>otherLetters</val>: anything other than jers,
                                        nasal vowels, and jotation.</item>
                                </list> Jer and nasal vowel specifications are formalized through
                                the <att>subtype</att> attribute, about shich see the examples
                                above.</p>
                        </remarks>
                    </elementSpec>

                    <elementSpec ident="orthography" ns="http://www.ilit.bas.bg/repertorium/ns/3.0"
                        mode="add">
                        <desc>Contains a formal, structured description of the orthography of a
                            scribe.</desc>
                        <classes mode="replace">
                            <memberOf key="att.global"/>
                            <memberOf key="att.typed"/>
                        </classes>
                        <content>
                            <rng:choice>
                                <rng:oneOrMore>
                                    <rng:ref name="model.pLike"/>
                                </rng:oneOrMore>
                                <rng:group>
                                    <rng:optional>
                                        <rng:ref name="summary"/>
                                    </rng:optional>
                                    <rng:oneOrMore>
                                        <rng:ref name="orthNote"/>
                                    </rng:oneOrMore>
                                </rng:group>
                            </rng:choice>
                        </content>
                        <exemplum>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples"><orthography><orthNote
                                        type="jer" subtype="front">only <c xml:lang="cu">ь</c>
                                    </orthNote><orthNote type="jus" subtype="nonConsis"><c
                                            xml:lang="cu">ѫ</c> replaced by <c xml:lang="cu">оу</c>;
                                            <c xml:lang="cu">ѧ</c> replaced by <c xml:lang="cu"
                                            >a</c>;</orthNote><orthNote type="otherLetters"><c
                                            xml:lang="cu">ы</c>; <c xml:lang="cu">ѥ</c> and <c
                                            xml:lang="cu">ѩ</c></orthNote></orthography></egXML>
                        </exemplum>
                        <remarks>
                            <p>The individual orthographic features are specified in
                                    <gi>orthoNote</gi> children of <gi>orthography</gi> (see the
                                separate documentation of <gi>orthNote</gi>).</p>
                        </remarks>
                    </elementSpec>

                    <elementSpec ident="pagination" ns="http://www.ilit.bas.bg/repertorium/ns/3.0"
                        mode="add">
                        <desc>Contains information about the pagination of the codex, separately
                            from information about the foliation.</desc>
                        <classes mode="change">
                            <memberOf key="att.global"/>
                            <memberOf key="att.scoping"/>
                        </classes>
                        <content>
                            <rng:ref name="macro.specialPara"/>
                        </content>
                        <exemplum>
                            <p>The following example is from the electronic description of MS
                                Pogodin 1204 by Antoaneta Granberg.</p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples"><foliation>original,
                                    from <date>1805</date></foliation></egXML>
                        </exemplum>
                        <remarks>
                            <p>The Repertorium adds a <gi>pagination</gi> element so that pagination
                                and foliation (represented by the standard TEI <gi>foliation</gi>
                                element) can be described separately. The <gi>pagination</gi>
                                element, like the <gi>foliation</gi> element, may include
                                information about when the pagination was made, whether it is
                                original, and how the folios are marked or numbered.</p>
                        </remarks>
                    </elementSpec>

                    <elementSpec ident="physDesc" mode="change" module="msdescription">
                        <content>
                            <rng:group>
                                <rng:zeroOrMore>
                                    <rng:ref name="model.pLike"/>
                                </rng:zeroOrMore>
                                <rng:group>
                                    <rng:optional>
                                        <rng:ref name="objectDesc"/>
                                    </rng:optional>
                                    <rng:optional>
                                        <rng:ref name="ink"/>
                                    </rng:optional>
                                    <rng:optional>
                                        <rng:ref name="decoDesc"/>
                                    </rng:optional>
                                    <rng:optional>
                                        <rng:ref name="bindingDesc"/>
                                    </rng:optional>
                                    <rng:optional>
                                        <rng:ref name="sealDesc"/>
                                    </rng:optional>
                                    <rng:zeroOrMore>
                                        <rng:ref name="additions"/>
                                    </rng:zeroOrMore>
                                </rng:group>
                            </rng:group>
                        </content>
                        <exemplum>
                            <p>TEI example:</p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples">
                                <physDesc>
                                    <objectDesc form="codex">
                                        <supportDesc material="perg">
                                            <support>Parchment.</support>
                                            <extent>i + 55 leaves <dimensions scope="all"
                                                  type="leaf" unit="inch">
                                                  <height>7¼</height>
                                                  <width>5⅜</width>
                                                </dimensions>
                                            </extent>
                                        </supportDesc>
                                        <layoutDesc>
                                            <layout columns="2">In double columns.</layout>
                                        </layoutDesc>
                                    </objectDesc>
                                    <handDesc>
                                        <p>Written in more than one hand.</p>
                                    </handDesc>
                                    <decoDesc>
                                        <p>With a few coloured capitals.</p>
                                    </decoDesc>
                                </physDesc>
                            </egXML>
                        </exemplum>
                        <exemplum>
                            <p>Repertorium totally removes elements <gi>handDesc</gi>,
                                    <gi>typeDesc</gi>. The element <gi>scriptDesc</gi> is moved to
                                the contents of element <gi>scribeDesc</gi>. The element
                                    <gi>accMat</gi> becomes part of the <gi>history</gi>. One more
                                is included from the Repertorium namespace: <gi>ink</gi>. An example
                                from DRA787a10Stock.xml: </p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples">
                                <physDesc>
                                    <objectDesc form="fragment">
                                        <supportDesc material="parchment">
                                            <support>
                                                <material>Parchment of very fine quality. Ff.1r-2v
                                                  and 2r-v (2 bifolia) both represents F-H, H-F
                                                  sides sequences.</material>
                                            </support>
                                            <extent>
                                                <measure unit="folia">4</measure>
                                                <dimensions unit="mm" type="folia">
                                                  <height>ca218</height>
                                                  <width>ca310</width>
                                                </dimensions>
                                            </extent>
                                            <foliation>Ff. 1v-8v are signed in pencil as follows:
                                                10:A, 10: B, 10: C, 10: D in the upper right-hand
                                                corner. </foliation>
                                            <condition>It is a 4-leaf fragment composing to bifolia,
                                                as ff. 1 and 2 are detached. The parcment has
                                                retained much of its original light colour and the
                                                original size. Ff. 1r, 2v, 3r and 3v have served as
                                                document covers and are darker. There are number of
                                                stains. </condition>
                                        </supportDesc>
                                        <layoutDesc>
                                            <layout columns="1" writtenLines="18" ruledLines="18"/>
                                        </layoutDesc>
                                    </objectDesc>
                                    <ink xmlns="http://www.ilit.bas.bg/repertorium/ns/3.0">Brownish
                                        for the main text, red and gold for the titles and the
                                        initials.</ink>
                                    <decoDesc>
                                        <p>Intials <foreign xml:lang="cu">В</foreign> on f. 1r - 55
                                            mm (5 lines) of heigh, almost outside the text column,
                                                <foreign xml:lang="cu">В</foreign> on f. 2r - 45mm
                                            of height, being outside the text column and discending
                                            in the lower margin, <foreign xml:lang="cu">Р</foreign>
                                            on f. 5v - 55 mm (5 lines) of heigh, outside the text
                                            column, and <foreign xml:lang="cu">Р</foreign> on ff. 8v
                                            - 55 mm (5 lines) of heigh, outside the text column,
                                            too. These are red covered with gold. The same technique
                                            is applied to the rubrics (titles) introducing the
                                            coresponding readings.</p>
                                    </decoDesc>
                                    <bindingDesc>
                                        <binding>
                                            <p>Kept together with other parchment fragments
                                                (A787a)</p>
                                        </binding>
                                    </bindingDesc>
                                    <additions type="inscriptions">
                                        <list>
                                            <item>
                                                <locus>f. 1r</locus>
                                                <quote xml:lang="se">Lille Savolax p anno 1618
                                                  <lb/>Sigfrids Hindersons Opbördzlängds</quote>
                                            </item>
                                            <item>
                                                <locus>f. 3r</locus>
                                                <quote xml:lang="se">Sigfred Henricksons Reckenskap
                                                  <lb/>på Viborgz Lähn af Lille Sauolax för årh
                                                  <lb/>1617, 618 och 619</quote>
                                            </item>
                                        </list>
                                    </additions>
                                </physDesc>
                            </egXML>
                        </exemplum>
                    </elementSpec>

                    <elementSpec ident="pricking" ns="http://www.ilit.bas.bg/repertorium/ns/3.0"
                        mode="add">
                        <desc>Contains a description of pricking patterns in the manuscript.</desc>
                        <classes mode="change">
                            <memberOf key="att.global"/>
                            <memberOf key="att.typed"/>
                        </classes>
                        <content>
                            <rng:ref name="macro.phraseSeq"/>
                        </content>
                        <exemplum>
                            <p>The following example is from DUL MS Cosin V.II.17 Anseis de
                                Carthage, Ogier le Danois (in French) s. xiii2 description (<ptr
                                    target="http://www.dur.ac.uk/library/asc/theme/medmss/apvii17/"
                                />), encoded according to the Repertorium model.</p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples"><pricking>Pricking in
                                    outer margins only, probably through folded quires, from front
                                    to back.</pricking></egXML>
                        </exemplum>
                        <exemplum>
                            <p> The following example is from the web-based electronic description
                                of St. Alban’s Psalter (<ptr
                                    target="http://www.abdn.ac.uk/~lib399/english/essays/codicology.shtml"
                                />), encoded according to the Repertorium model.</p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples">
                                <pricking>Most prick marks have been trimmed off. On p 112, 6 prick
                                    marks are visible on the outer margin. On p 417, thirteen prick
                                    marks at 9mm intervals remain on the outer margin. There is no
                                    pricking on the inner margins.</pricking></egXML>
                        </exemplum>
                        <remarks>
                            <p>Descriptions of pricking are rare in the literature, although, as
                                explained by Jones (1946a, 1946b), they could bear information on
                                scriptorial techniques.</p>
                        </remarks>
                    </elementSpec>

                    <elementSpec ident="quire" ns="http://www.ilit.bas.bg/repertorium/ns/3.0"
                        mode="add">
                        <desc>Conains information that describes a quire or set of quires.</desc>
                        <classes mode="change">
                            <memberOf key="att.global"/>
                            <memberOf key="att.typed"/>
                        </classes>
                        <content>
                            <rng:choice>
                                <rng:oneOrMore>
                                    <rng:ref name="model.pLike"/>
                                </rng:oneOrMore>
                                <rng:group>
                                    <rng:optional>
                                        <rng:ref name="summary"/>
                                    </rng:optional>
                                    <rng:optional>
                                        <rng:ref name="locus"/>
                                    </rng:optional>
                                    <rng:optional>
                                        <rng:ref name="locusGrp"/>
                                    </rng:optional>
                                    <rng:optional>
                                        <rng:ref name="extent"/>
                                    </rng:optional>
                                    <rng:optional>
                                        <rng:ref name="formula"/>
                                    </rng:optional>
                                    <rng:optional>
                                        <rng:ref name="signatures"/>
                                    </rng:optional>
                                    <rng:optional>
                                        <rng:ref name="catchwords"/>
                                    </rng:optional>
                                    <rng:optional>
                                        <rng:ref name="gregoryRule"/>
                                    </rng:optional>
                                    <rng:optional>
                                        <rng:ref name="ruling"/>
                                    </rng:optional>
                                    <rng:optional>
                                        <rng:ref name="pricking"/>
                                    </rng:optional>
                                </rng:group>
                            </rng:choice>
                        </content>
                        <attList>
                            <attDef ident="status" usage="opt">
                                <desc>Describes the status of the quire or folia: original, added,
                                    missing</desc>
                                <datatype>
                                    <rng:ref name="datatype.Token"/>
                                </datatype>
                                <valList type="closed" mode="add">
                                    <valItem ident="missing">
                                        <gloss>Original quire is missing</gloss>
                                    </valItem>
                                    <valItem ident="added">
                                        <gloss>New quire has been added</gloss>
                                    </valItem>
                                    <valItem ident="original">
                                        <gloss>Quire is original</gloss>
                                    </valItem>
                                </valList>
                            </attDef>
                        </attList>
                        <exemplum>
                            <p>In the following example, the first quire of the manuscript consists
                                of 8 ff., all of which, with the exception of the f. 7, are now
                                missing. Quires II through XXI have 8 ff. each. From quire XXII one
                                folio (7) is missing. The last quire, number XXIII, consisted
                                originally of 6 ff., but only 2 of them (5 and 6) are preserved
                                intact.</p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples">
                                <collation>
                                    <quire n="I"><extent>2</extent><formula>8 (-1-2-3-4-5-6)
                                        </formula></quire>
                                    <quire n="II-XXI"><extent>8</extent></quire>
                                    <quire n="XXII"><extent>7</extent><formula>7 (-7)</formula>
                                    </quire>
                                    <quire n="XXIII"><extent>2</extent><formula>6 (-1-2-3-4)
                                        </formula></quire>
                                </collation>
                            </egXML>
                        </exemplum>
                        <remarks>
                            <p>Relevant information includes number, composition, how and where
                                quires are marked, and with the assistance of what tool, etc. The
                                child element <gi>formula</gi> provides information about how the
                                quire is composed, of how many folia, etc.</p>
                        </remarks>
                    </elementSpec>

                    <elementSpec ident="ruling" ns="http://www.ilit.bas.bg/repertorium/ns/3.0"
                        mode="add">
                        <desc>Contains information about ruling lines in the manuscript.</desc>
                        <classes mode="change">
                            <memberOf key="att.global"/>
                            <memberOf key="att.typed"/>
                        </classes>
                        <content>
                            <rng:ref name="macro.phraseSeq"/>
                        </content>
                        <exemplum>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples"><ruling>
                                    <formula>1+1 1/1</formula>
                                </ruling></egXML>
                        </exemplum>
                        <remarks>
                            <p>Two kinds of information can be entered in the <gi>ruling</gi>
                                element. The first is related to the ruling type defined by
                                researchers like Leroy (1976) or Gilissen (1977), who attempted to
                                propose well-defined typologies of ruling types. In the other,
                                information is related to the place of ruling in the quire, or the
                                so called <q>scheme</q>. The former be expressed as a formula of
                                ruled lines for writing, and the latter could be visualized as
                                simple graphic showing the places in the quire where the ruling is
                                accomplished, e.g.: <q>&gt;&gt; | &gt; | |&lt; |&lt;&lt; = &gt;&gt;|
                                    &gt;| |&lt; | &lt;&lt;</q>. This scheme means that in this quire
                                the ruling is made for each of the two folia: once for the outer
                                double leaf and once for the inner one (double sign <q>&gt;&gt;</q>
                                or <q>&lt;&lt;</q>). The symbols <q>&gt;</q> or <q>&lt;</q> mean
                                that on the leaf there is just a trace of the ruling made on the
                                previous leaf. The attribute <att>type</att> can be used to provide
                                information about the classification scheme (e.g., Gilissen or
                                Leroy) employed.</p>
                        </remarks>
                    </elementSpec>

                    <elementSpec ident="sampleText" ns="http://www.ilit.bas.bg/repertorium/ns/3.0"
                        mode="add">
                        <desc>Wraps samples from the manuscript, both those with specific identities
                            (such as <gi>head</gi>, <gi>incipit</gi>, etc.) and general ones (tagged
                            as <gi>p</gi>).</desc>
                        <classes mode="change">
                            <memberOf key="att.divLike"/>
                            <memberOf key="att.declaring"/>
                            <memberOf key="att.global"/>
                            <memberOf key="att.typed"/>
                        </classes>
                        <content>
                            <rng:zeroOrMore>
                                <rng:choice>
                                    <rng:ref name="head"/>
                                    <rng:ref name="rubric"/>
                                    <rng:ref name="incipit"/>
                                </rng:choice>
                            </rng:zeroOrMore>
                            <rng:zeroOrMore>
                                <rng:choice>
                                    <rng:ref name="model.pLike"/>
                                    <rng:ref name="div"/>
                                </rng:choice>
                            </rng:zeroOrMore>
                            <rng:zeroOrMore>
                                <rng:choice>
                                    <rng:ref name="explicit"/>
                                    <rng:ref name="finalRubric"/>
                                </rng:choice>
                            </rng:zeroOrMore>
                        </content>
                        <attList>
                            <attDef ident="script" usage="opt">
                                <datatype>
                                    <rng:text/>
                                </datatype>
                            </attDef>
                        </attList>
                        <exemplum>
                            <p>The following example is from the electronic description of
                                DA1039NBKM.xml.</p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples"><sampleText
                                    xml:lang="cu" script="cyrs">
                                    <head>Mсц҃а тогожде · е҃· дн҃ь · словѡ · ѡ рождьствѣ іѡана
                                        прт҃че и ѡ оумрьтїоу ѡц҃а его захарїе ѡч҃е бл҃сви · </head>
                                    <incipit>Вь лѣто четврьтодесетноѥ црс҃твоующоу иродоу вьзысканиѥ
                                        сьтвори изьбити млд҃нце, иже вь іерс҃лимѣ</incipit>
                                    <explicit>.црс҃твова же архела ѳ҃ лѣт҃ испльнь. вь нас же
                                        црс҃твоующоу бо҃у нашемоу ісо҃у хс҃оу· емоу же слав(а) сь
                                        ѡц҃емь и сь ст҃мь дх҃омь нн҃ꙗ и присно и в вѣкы вѣкѡм(ь)
                                        аминь.</explicit>
                                </sampleText></egXML>
                        </exemplum>
                        <remarks>
                            <p>The Repertorium uses a special <gi>sampleText</gi> element as a
                                wrapper around several elements used for the identification of text
                                items in a manuscript. Its function is to make a distinction between
                                the language of the description (metadata used to describe the
                                manuscript) and citations from the original text. The
                                    <gi>sampleText</gi> element uses the global TEI attribute
                                    <att>xml:lang</att>, which represents the language of the
                                content, and for medieval Slavonic codices its value is almost
                                always <val>cu</val>. The optional attribute <att>script</att>
                                commonly contains the value <val>cyrs</val> (for old Cyrillic) or
                                    <val>glag</val> (for Glagolitic).</p>
                        </remarks>
                    </elementSpec>

                    <elementSpec ident="scribe" ns="http://www.ilit.bas.bg/repertorium/ns/3.0"
                        mode="add">
                        <desc>Contains a description of the paleography, orthography, and language
                            of a scribe.</desc>
                        <classes mode="change">
                            <memberOf key="att.global"/>
                        </classes>
                        <content>
                            <rng:optional>
                                <rng:choice>
                                    <rng:ref name="locus"/>
                                    <rng:ref name="locusGrp"/>
                                </rng:choice>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="model.nameLike.agent"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="scriptDesc"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="scribeLang"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="bibl"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="note"/>
                            </rng:optional>
                            <rng:zeroOrMore>
                                <rng:ref name="respStmt"/>
                            </rng:zeroOrMore>
                        </content>
                        <exemplum>
                            <p>In the example below, the portions of the manuscript written by
                                Scribe E are identified in a <gi>locusGrp</gi>, followed by a
                                general characterization of the paleography
                                (<gi>palaeoCharact</gi>). See the separate documentation of the
                                Repertorium <gi>scribeLang</gi>, <gi>orthography</gi>, and
                                    <gi>orthNote</gi> elements.</p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples"><scribe n="E"
                                            ><locusGrp><locus>Q.п.I.63, 1r-3r</locus>
                                        <locus>Sin. 34, 63v-71v</locus><locus>Sin. 34, 77r-81r
                                        </locus></locusGrp>
                                    <palaeoCharact script="cyrs"><p>Uncial</p></palaeoCharact>
                                    <scribeLang><summary>Middle Bulgarian with Russian
                                            features</summary><orthography><orthNote type="jer"
                                                subtype="front">only <c xml:lang="cu">ь</c>
                                            </orthNote><orthNote type="jus" subtype="nonConsis"><c
                                                  xml:lang="cu">ѫ</c> replaced by <c xml:lang="cu"
                                                  >оу</c>; <c xml:lang="cu">ѧ</c> replaced by <c
                                                  xml:lang="cu">a</c>;</orthNote><orthNote
                                                type="otherLetters"><c xml:lang="cu">ы</c>; <c
                                                  xml:lang="cu">ѥ</c> and <c xml:lang="cu"
                                                >ѩ</c></orthNote></orthography> …
                                    </scribeLang></scribe></egXML>
                        </exemplum>
                        <remarks>
                            <p>The <gi>scribe</gi> element combines a modification of the TEI
                                    <gi>scriptDesc</gi> element with the Repertorium-specific
                                    <gi>scribeLang</gi> element.</p>
                        </remarks>
                    </elementSpec>

                    <elementSpec ident="scribeDesc" ns="http://www.ilit.bas.bg/repertorium/ns/3.0"
                        mode="add">
                        <desc>Serves as a wrapper for <gi>scribe</gi> elements, so that the
                            characteristics of each scribe can be described separately, while also
                            being bundled together.</desc>
                        <classes mode="change">
                            <memberOf key="att.global"/>
                        </classes>
                        <content>
                            <rng:choice>
                                <rng:oneOrMore>
                                    <rng:ref name="model.pLike"/>
                                </rng:oneOrMore>
                                <rng:group>
                                    <rng:optional>
                                        <rng:ref name="summary"/>
                                    </rng:optional>
                                    <rng:zeroOrMore>
                                        <rng:ref name="scribe"/>
                                    </rng:zeroOrMore>
                                </rng:group>
                            </rng:choice>
                        </content>
                        <exemplum>
                            <p>The following example, from AA771NBKM.xml, contains information about
                                two scribes.</p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples"><scribeDesc>
                                    <scribe n="1">
                                        <locusGrp>
                                            <locus>ff. 27–30</locus>
                                            <locus>ff. 48–50</locus>
                                            <locus>f. 54</locus>
                                            <locus>ff. 61–62</locus>
                                        </locusGrp>
                                        <name full="yes">anonymous</name>
                                        <scriptDesc script="cyrs" type="semiuncial"/>
                                        <scribeLang>
                                            <orthography>
                                                <p>Two nasal vowels and two jers</p>
                                            </orthography>
                                        </scribeLang>
                                        <respStmt>
                                            <name full="yes">Adelina Angusheva</name>
                                            <resp>de visu</resp>
                                        </respStmt>
                                    </scribe>
                                    <scribe n="2">
                                        <locusGrp>
                                            <locus>ff. 6–16</locus>
                                            <locus>ff. 18–21</locus>
                                            <locus>ff. 33–34</locus>
                                            <locus>ff. 38–42</locus>
                                            <locus>ff. 44–45</locus>
                                            <locus>ff. 51</locus>
                                            <locus>ff. 53</locus>
                                            <locus>ff. 71</locus>
                                            <locus>ff. 78</locus>
                                            <locus>ff. 91–92</locus>
                                            <locus>f. 109</locus>
                                            <locus>f. 131</locus>
                                        </locusGrp>
                                        <name full="yes">anonymous</name>
                                        <scriptDesc script="cyrs" type="semiuncial"/>
                                        <respStmt>
                                            <name full="yes">Adelina Angusheva</name>
                                            <resp>de visu</resp>
                                        </respStmt>
                                    </scribe>
                                </scribeDesc>
                            </egXML>
                        </exemplum>
                    </elementSpec>

                    <elementSpec ident="scribeLang" ns="http://www.ilit.bas.bg/repertorium/ns/3.0"
                        mode="add">
                        <desc>Contains a description of the orthography and language of the
                            scribe.</desc>
                        <classes mode="change">
                            <memberOf key="att.global"/>
                            <memberOf key="att.typed"/>
                        </classes>
                        <content>
                            <rng:choice>
                                <rng:oneOrMore>
                                    <rng:ref name="model.pLike"/>
                                </rng:oneOrMore>
                                <rng:group>
                                    <rng:optional>
                                        <rng:ref name="summary"/>
                                    </rng:optional>
                                    <rng:optional>
                                        <rng:ref name="orthography"/>
                                    </rng:optional>
                                    <rng:optional>
                                        <rng:ref name="grammar"/>
                                    </rng:optional>
                                    <rng:optional>
                                        <rng:ref name="lexis"/>
                                    </rng:optional>
                                </rng:group>
                            </rng:choice>
                        </content>
                        <attList>
                            <attDef ident="script" usage="opt">
                                <datatype>
                                    <rng:text/>
                                </datatype>
                            </attDef>
                        </attList>
                        <exemplum>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples"
                                        ><scribeLang><summary>Middle Bulgarian with Russian
                                        features</summary><orthography><orthNote type="jer"
                                            subtype="front">only <c xml:lang="cu">ь</c>
                                        </orthNote><orthNote type="jus" subtype="nonConsis"><c
                                                xml:lang="cu">ѫ</c> replaced by <c xml:lang="cu"
                                                >оу</c>; <c xml:lang="cu">ѧ</c> replaced by <c
                                                xml:lang="cu">a</c>;</orthNote><orthNote
                                            type="otherLetters"><c xml:lang="cu">ы</c>; <c
                                                xml:lang="cu">ѥ</c> and <c xml:lang="cu"
                                            >ѩ</c></orthNote></orthography> … </scribeLang></egXML>
                        </exemplum>
                        <remarks>
                            <p>The <gi>scribeLang</gi> element contains a formal description of the
                                language of a scribe, expressed in the <gi>orthography</gi>,
                                    <gi>grammar</gi>, and <gi>lexis</gi> child elements.</p>
                        </remarks>
                    </elementSpec>

                    <elementSpec ident="scriptDesc" mode="change">
                        <desc>Repertorium model: Paleographic characteristics</desc>
                        <classes mode="change">
                            <memberOf key="att.global.attributes"/>
                            <memberOf key="att.handFeatures.attributes"/>
                            <memberOf key="att.typed"/>
                        </classes>
                        <content>
                            <rng:choice>
                                <rng:oneOrMore>
                                    <rng:ref name="model.pLike"/>
                                </rng:oneOrMore>
                                <rng:group>
                                    <rng:optional>
                                        <rng:ref name="summary"/>
                                    </rng:optional>
                                    <rng:zeroOrMore>
                                        <rng:ref name="scriptNote"/>
                                    </rng:zeroOrMore>
                                    <rng:zeroOrMore>
                                        <rng:ref name="musicNotation"/>
                                    </rng:zeroOrMore>
                                    <rng:optional>
                                        <rng:ref name="crypto"/>
                                    </rng:optional>
                                </rng:group>
                            </rng:choice>
                        </content>
                        <attList>
                            <attDef ident="script" usage="opt">
                                <datatype>
                                    <rng:text/>
                                </datatype>
                            </attDef>
                        </attList>
                        <exemplum>
                            <p>TEI example: </p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples">
                                <scriptDesc>
                                    <summary>Contains two distinct styles of scripts </summary>
                                    <scriptNote xml:id="style-1">.</scriptNote>
                                    <scriptNote xml:id="style-2">.</scriptNote>
                                </scriptDesc>
                            </egXML>
                        </exemplum>
                        <exemplum>
                            <p>Repertorium adds optional attribute <att>script</att> the values of
                                which are supposed to come from codes of ISO 15924:2004, cf <ref
                                    target="https://en.wikipedia.org/wiki/ISO_15924"
                                    >https://en.wikipedia.org/wiki/ISO_15924</ref>. In the content
                                of this element <gi>musicNotation</gi> is moved here from
                                    <gi>physDesc</gi> and a new <gi>crypto</gi> Repertorium is
                                added. An example from the file AA771NBKM.xml </p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples">
                                <scriptDesc script="cyrs" type="semiuncial"/>
                            </egXML>
                        </exemplum>
                    </elementSpec>

                    <elementSpec ident="scriptNote" mode="change">
                        <classes mode="change">
                            <memberOf key="att.typed"/>
                        </classes>
                        <content>
                            <rng:ref name="macro.specialPara"/>
                        </content>
                        <exemplum>
                            <p>TEI example:</p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples">
                                <scriptNote scope="sole"/>
                            </egXML>
                        </exemplum>
                        <exemplum>
                            <p>Repertorium adds <att>type</att> to the known TEI attributes. </p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples"> To be added ...
                            </egXML>
                        </exemplum>
                    </elementSpec>

                    <elementSpec ident="supportDesc" module="msdescription" mode="change">
                        <content>
                            <rng:choice>
                                <rng:oneOrMore>
                                    <rng:ref name="model.pLike"/>
                                </rng:oneOrMore>
                                <rng:group>
                                    <rng:optional>
                                        <rng:ref name="support"/>
                                    </rng:optional>
                                    <rng:optional>
                                        <rng:ref name="extent"/>
                                    </rng:optional>
                                    <rng:zeroOrMore>
                                        <rng:choice>
                                            <rng:ref name="foliation"/>
                                            <rng:ref name="pagination"/>
                                        </rng:choice>
                                    </rng:zeroOrMore>
                                    <rng:optional>
                                        <rng:ref name="collation"/>
                                    </rng:optional>
                                    <rng:optional>
                                        <rng:ref name="condition"/>
                                    </rng:optional>
                                </rng:group>
                            </rng:choice>
                        </content>
                        <exemplum>
                            <p>TEI example:</p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples">
                                <supportDesc>
                                    <support> Parchment roll with <material>silk</material> ribbons.
                                    </support>
                                </supportDesc>
                            </egXML>
                        </exemplum>
                        <exemplum>
                            <p>Repertorium adds a new element <gi>pagination</gi> after the
                                    <gi>foliation</gi>. An example from MP408G.xml: </p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples">
                                <supportDesc material="paper">
                                    <support>
                                        <material>Paper</material>
                                        <watermark>
                                            <motif xmlns="http://www.ilit.bas.bg/repertorium/ns/3.0"
                                                >Letter P</motif>
                                            <ref type="bibl" target="bib:Mošin????">
                                                <num>5449</num>
                                                <date>?</date>
                                            </ref>
                                        </watermark>
                                        <watermark>
                                            <motif xmlns="http://www.ilit.bas.bg/repertorium/ns/3.0"
                                                >Clock</motif>
                                            <ref type="bibl" target="bib:Mošin????">
                                                <num>3002</num>
                                                <date>?</date>
                                            </ref>
                                        </watermark>
                                        <watermark>
                                            <motif xmlns="http://www.ilit.bas.bg/repertorium/ns/3.0"
                                                >Hills</motif>
                                            <ref type="bibl" target="bib:Mošin????">
                                                <num>6255</num>
                                                <date>?</date>
                                            </ref>
                                        </watermark>
                                    </support>
                                    <extent>
                                        <measure unit="folia">242</measure>
                                        <dimensions unit="mm" type="folia">
                                            <height>200</height>
                                            <width>130</width>
                                        </dimensions>
                                    </extent>
                                    <pagination xmlns="http://www.ilit.bas.bg/repertorium/ns/3.0"
                                        >Not original, J. de Saint-Genois for every page =
                                        474</pagination>
                                    <collation>
                                        <summary>31 quires of 8 folia</summary>
                                    </collation>
                                    <condition>f. 5v-6, ff.101v-102, ff. 243-244 are
                                        damaged</condition>
                                </supportDesc>
                            </egXML>
                        </exemplum>
                    </elementSpec>

                    <elementSpec ident="watermark" module="msdescription" mode="change">
                        <desc>The content of this element includes phrase level elements, as well as
                            specialized elements like <gi>countermark</gi> and <gi>motif</gi>. There
                            are several special attributes for the content of this element.
                                <att>tracing</att> is used to describe very briefly (with one word)
                            the method of watermark tracing. The most usual method is still drawing
                            by hand, but there could be other possibilities, such as rubbing by
                            hand, usage of special Dylux print paper, beta- or x-radiography,
                            scanning, etc. The <att>place</att> attribute from the class
                            att.placement can refine where on the pages the watermark can be seen,
                            and may be described in relation to wire lines and chain (laid)
                            lines.</desc>
                        <content>
                            <rng:zeroOrMore>
                                <rng:choice>
                                    <rng:text/>
                                    <rng:ref name="model.phrase"/>
                                    <rng:ref name="countermark"/>
                                    <rng:ref name="motif"/>
                                </rng:choice>
                            </rng:zeroOrMore>
                        </content>
                        <attList>
                            <attDef ident="tracing">
                                <desc>describes the methods of recording of some watermark. Methods
                                    include: tracing, rubbing, etc.</desc>
                                <datatype>
                                    <rng:text/>
                                </datatype>
                            </attDef>
                        </attList>
                        <exemplum>
                            <p>TEI example: </p>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples">
                                <objectDesc form="codex">
                                    <supportDesc>
                                        <p>Mostly <material>paper</material>, with watermarks
                                                <watermark>unicorn</watermark> (<ref>Briquet
                                                9993</ref>) and <watermark>ox</watermark> (close to
                                                <ref>Briquet 2785</ref>). The first and last leaf of
                                            each quire, with the exception of quires xvi and xviii,
                                            are constituted by bifolia of parchment, and all seven
                                            miniatures have been painted on inserted singletons of
                                            parchment.</p>
                                    </supportDesc>
                                </objectDesc>
                            </egXML>
                        </exemplum>
                        <exemplum>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples">
                                <watermark tracing="drawing"><motif type="basic">Leo</motif>within
                                        <motif type="supplemental">circle</motif> surmounted by
                                        <motif type="secondary">quatrefoil</motif> and with
                                        <countermark>“AP”</countermark> similar to <ref type="bibl"
                                        target="bib:Николаев1954">Nikolaev #125</ref> from the year
                                        <date cert="yes">1585</date>. </watermark>
                            </egXML>
                        </exemplum>
                    </elementSpec>

                </schemaSpec>
            </div>
        </body>
    </text>
</TEI>
