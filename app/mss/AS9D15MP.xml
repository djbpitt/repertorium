                    <origin evidence="conjecture" resp="#AS">
                        <origDate notBefore="1501" notAfter="1599">16th c.</origDate>
                        <ref type="bibl" target="bib:Vašica1957">Vašica &amp; Vajs 1957:
                            112–114</ref>
                    </origin>
                </history>
            </msDesc>
        </body>
    </text>
