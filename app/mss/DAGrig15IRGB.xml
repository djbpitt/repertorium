                        <locus xmlns="http://www.tei-c.org/ns/1.0">ff. 1r-2v</locus>
                        <name xmlns="http://www.tei-c.org/ns/1.0" role="scribe">Theodor</name>
                        <scriptDesc xmlns="http://www.tei-c.org/ns/1.0">
                            <scriptNote script="Cyrs" style="uncial"/>
