                <physDesc>
                    <summary>The fragment belongs to a big codex, found from Grigorovič at the time of his
                        trip.</summary>
                    <objectDesc>
                        <supportDesc material="parch">
                            <support>
                                <material>Parchment, rough and uneven</material>
                            </support>
                            <extent>
                                <measure unit="folia">2</measure>
                                <dimensions type="folia" unit="mm">
                                    <height>240</height>
                                    <width>168</width>
                                </dimensions>
                            </extent>
                            <foliation>No foliation.</foliation>
                            <condition>The manuscript is damaged after scission; patches of
                                reagent.</condition>
                        </supportDesc>
                        <layoutDesc>
                            <layout columns="2" writtenLines="30 31"/>
                        </layoutDesc>
                    </objectDesc>
                    <ink xmlns="http://www.ilit.bas.bg/repertorium/ns/3.0">Dark brown</ink>
                    <decoDesc>
                        <decoNote type="initials">Initials of geometric type, cinnabar contour;
                            yellow decoration. </decoNote>
                    </decoDesc>
                    <bindingDesc>
                        <p>Without binding.</p>
                    </bindingDesc>
                </physDesc>
                <scribeDesc xmlns="http://www.ilit.bas.bg/repertorium/ns/3.0">
