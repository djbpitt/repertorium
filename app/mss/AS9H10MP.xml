                    <origin evidence="catalogue" resp="#AS">
                        <origDate notBefore="1701" notAfter="1799">18th c.</origDate>
                            <ref type="bibl" target="bib:Vašica1957">Vašica &amp; Vajs 1957:
                                314–317</ref>
                    </origin>
                </history>
            </msDesc>
        </body>
