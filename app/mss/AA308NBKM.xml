                    <msItemStruct xml:id="ACD1" type="original">
                        <locus n="1">ff. 1r-33v</locus>
                        <title xml:lang="bg">Сказание за буквите от Константин Костенечки</title>
                        <author>Constantine of Kostenets</author>
                        <note>Without beginning, fragment.</note>
                        <filiation type="protograph">Bulgarian</filiation>
                        <filiation type="antigraph">Middle Bulgarian</filiation>
                        <filiation type="litRedaction">short</filiation>
                        <sampleText xmlns="http://www.ilit.bas.bg/repertorium/ns/3.0" xml:lang="cu">
                            <explicit xmlns="http://www.tei-c.org/ns/1.0">кр<seg rend="sup">с</seg>те помагаи а не кр<seg rend="sup">с</seg>те б҃е пом<seg rend="sup">г</seg>аи. кр<seg rend="sup">с</seg>ть бѡ нѣ<seg rend="sup">с</seg> б҃ь нь знаменїе б҃зїе на бѣсы.</explicit>
                        </sampleText>
                    </msItemStruct>
                    <msItemStruct xml:id="ACD2" type="translation" style="non-narrative">
