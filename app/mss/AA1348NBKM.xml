                    <note>
                        <persName type="relatedPerson">Петарь Боженіца</persName>
                        <ref type="bibl" target="bib:Стоянов1971">Стоянов, Кодов 1971:141</ref>
                    </note>
                </msIdentifier>
                <physDesc>
                    <objectDesc form="codex">
                        <supportDesc material="paper">
                            <support>
                                <watermark>None</watermark>
                            </support>
