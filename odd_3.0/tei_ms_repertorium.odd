<TEI xmlns:xi="http://www.w3.org/2001/XInclude"
    xmlns:svg="http://www.w3.org/2000/svg"
    xmlns:math="http://www.w3.org/1998/Math/MathML"
    xmlns="http://www.tei-c.org/ns/1.0"
    xmlns:re="http://www.ilit.bas.bg/repertorium/ns/3.0"
    xmlns:rng="http://relaxng.org/ns/structure/1.0" 
    xml:lang="en"
    n="repertorium">
    <teiHeader>
        <fileDesc>
            <titleStmt>
                <title>Repertorium MS model based on TEI P5 (version 2.3.0)</title>
                <author xml:id="atb">Andrej Bojadziev</author>
            </titleStmt>
            <editionStmt>
                <edition>3.0 Last updated 23.04.2013</edition>
            </editionStmt>
            <publicationStmt>
<date when="2013-04-23">23 April 2013</date>
                <availability status="free">
                    <p>
