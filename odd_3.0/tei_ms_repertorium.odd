                                    <rng:optional>
                                        <rng:ref name="summary" />
                                    </rng:optional>
                                    <rng:optional><rng:ref name="locus" /></rng:optional>
                                    <rng:optional><rng:ref name="locusGrp" /></rng:optional>
                                    <rng:optional><rng:ref name="extent" /></rng:optional>
                                    <rng:optional><rng:ref name="formula" /></rng:optional>
